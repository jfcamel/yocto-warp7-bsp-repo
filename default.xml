  <project remote="warp7" revision="master" name="meta-warp7-distro" path="sources/meta-warp7-distro"/>
  <!-- Qt5 -->
  <project remote="qt5" revision="master" name="meta-qt5" path="sources/meta-qt5"/>
  <!-- SWUpdate -->
  <project remote="swupdate" revision="master" name="meta-swupdate" path="sources/meta-swupdate"/>
  <!-- Mender -->
  <project remote="mender" revision="master" name="meta-mender" path="sources/meta-mender"/>

